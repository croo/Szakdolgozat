<?xml version="1.0"?>

<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:georss="http://www.georss.org/georss#"
    xmlns:foaf="http://xmlns.com/foaf/0.1/"
    xmlns:croo="http://example.org/croo#"
    xmlns:geo="http://www.w3.org/2003/01/geo/wgs84_pos#">
  <croo:Route rdf:about="http://example.org/base#frombudapesttoesztergom">
    <georss:line>54.6,54.701 54.701,54.701 54.701,54.701</georss:line>
    <croo:endTown>
      <croo:Town rdf:about="http://example.org/base#esztergom">
        <geo:long>18.7435935</geo:long>
        <geo:lat>47.7776069</geo:lat>
        <foaf:Name>esztergom</foaf:Name>
      </croo:Town>
    </croo:endTown>
    <croo:startTown>
      <croo:Town rdf:about="http://example.org/base#budapest">
        <geo:long>19.04076</geo:long>
        <geo:lat>47.49841</geo:lat>
        <foaf:Name>budapest</foaf:Name>
      </croo:Town>
    </croo:startTown>
  </croo:Route>
  <croo:Route rdf:about="http://example.org/base#fromesztergomtobudapest">
    <georss:line>54.701,54.701 54.701,54.701 54.6,54.701</georss:line>
    <croo:endTown rdf:resource="http://example.org/base#budapest"/>
    <croo:startTown rdf:resource="http://example.org/base#esztergom"/>
  </croo:Route>
</rdf:RDF>